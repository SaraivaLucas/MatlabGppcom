    "from matplotlib import pyplot as plt\n",
    "import numpy as np\n",
    "x = np.linspace(0,150,300)\n",
    "alpha = float(input('choose alpha '))\n",
    "omega = float(input('choose angular frequency '))\n",
    "f = np.exp(alpha*x)*np.cos(x*omega)\n",
    "plt.plot(x,f)\n",
    "plt.show()"
   ]
